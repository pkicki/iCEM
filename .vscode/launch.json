    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "cwd": "${workspaceFolder}/icem",
            "justMyCode": false,
            "console": "integratedTerminal"
        },
        {
            "name": "MAIN",
            "type": "debugpy",
            "request": "launch",
            "program": "main.py",
            "cwd": "${workspaceFolder}/icem",
            "justMyCode": false,
            "console": "integratedTerminal",
            "args": [
                "./settings/halfcheetah_running/i-cem-blitz.json",
                "controller_params.action_sampler_params.noise_beta=0.2"
            ]
        },
    ]
}